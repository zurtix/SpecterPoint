{
  "build": {
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "client",
  "mainBinaryName": "client",
  "version": "0.0.0",
  "identifier": "com.specterpint.dev",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "SpecterPoint",
        "width": 800,
        "height": 600,
        "theme": "Dark",
        "transparent": true,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}